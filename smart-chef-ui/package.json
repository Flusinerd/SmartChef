{
  "name": "smart-chef-ui",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.0",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "cypress run-ct",
    "e2e-ci": "start-server-and-test \"react-scripts -r @cypress/instrument-cra start\" http://localhost:3000 \"yarn cypress run\"",
    "lint": "eslint ./src",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "merge-reports": "nyc merge ./.nyc_output ./coverage/report.json",
    "generate-coverage": "nyc report --reporter=html --reporter=text-summary"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:cypress/recommended"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/instrument-cra": "^1.4.0",
    "@cypress/react": "^5.12.4",
    "@cypress/webpack-dev-server": "^1.8.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@storybook/addon-actions": "^6.4.21",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-interactions": "^6.4.21",
    "@storybook/addon-links": "^6.4.21",
    "@storybook/builder-webpack5": "^6.4.21",
    "@storybook/manager-webpack5": "^6.4.21",
    "@storybook/node-logger": "^6.4.21",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.4.21",
    "@storybook/testing-library": "^0.0.9",
    "@testing-library/cypress": "^8.0.2",
    "cypress": "^9.5.3",
    "eslint": "^8.13.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.7.0",
    "webpack": "^5.72.0"
  }
}
